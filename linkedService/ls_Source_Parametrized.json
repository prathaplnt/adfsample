{
	"name": "ls_Source_Parametrized",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"parameters": {
			"ls_sourceSA_URL": {
				"type": "string"
			},
			"ls_akv_KeyVaultSecretName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "AzureBlobFS",
		"typeProperties": {
			"url": "@{linkedService().ls_sourceSA_URL}",
			"accountKey": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "ls_aux_akv_parametrization",
					"type": "LinkedServiceReference"
				},
				"secretName": {
					"value": "@linkedService().ls_akv_KeyVaultSecretName",
					"type": "Expression"
				}
			}
		}
	}
}